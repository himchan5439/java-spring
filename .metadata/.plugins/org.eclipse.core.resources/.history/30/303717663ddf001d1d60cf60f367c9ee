package ch14;

public class Subway {
	int SubwayNum;
	int count;
	int money;
	
	
	public Subway(int subwayNum) {
		this.busNum = subwayNum;
	}
	
	public void take(int money) {
		this.money += money;
		count++;
	}
	
	public void showSubwayInfo() {
		System.out.println(subwayNum + "번의 승객 수는 " + count + "명 이며 " + money + "원을 벌었습니다.");
	}
}
